.data
    vetor: .asciiz "0123456789ABCDEF"
    caractere: .asciiz "B"
    encontrou: .asciiz "Encontrou o caractere na posição: "
    naoEncontrou: .asciiz "Não encontrou o caractere."

.text
	li $t1, 0

	lb $t2, caractere
	
	procuraIgualdade:
		lb $t0, vetor($t1)
		
		beq $t0, $t2, encontrei
		
		addi $t1, $t1, 1
		
		j procuraIgualdade
		
	encontrei:
		li $v0, 1
		move $a0, $t1
		syscall
		